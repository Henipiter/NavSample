<resources>
    <!-- Base application theme. -->
    <style name="Base.Theme.NavSample" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorOnPrimary">@color/colorOnPrimary</item>
        <item name="colorError">@color/error_color</item>
        <item name="android:textColorHighlight">@color/colorOnPrimary</item>
        <item name="bottomSheetDialogTheme">@style/ThemeOverlay.App.BottomSheetDialog</item>
    </style>

    <color name="primaryColor">@color/orange_800</color>
    <color name="colorOnPrimary">@color/orange_1000</color>
    <color name="background_color">@color/basic_grey</color>
    <color name="sortingCardView">@color/mid_grey</color>
    <color name="popupCardView">@color/mid_grey</color>
    <color name="background_tile_chosen">@color/orange_500</color>
    <color name="background_tile_default">@color/mid_grey</color>
    <color name="background_tile_blocked">@color/mid_grey200</color>

</resources>